{
    "compilerOptions": {
        "rootDir": ".",
        "baseUrl": ".",
        "paths": {
            "src/*": [
                "src/*"
            ]
        },
        "lib": ["ES2020", "DOM"],
        "target": "ES2020",
        "allowJs": true,
        "checkJs": true,
        "outDir": "docs/",
        "module": "NodeNext",
        "resolveJsonModule": true,
        "esModuleInterop": true,
        "types": [
            "vite/client"
        ]
    },
    "exclude": [
        "node_modules",
        "docs"
    ]
}
